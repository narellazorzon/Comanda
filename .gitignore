# Dependencias de Composer
/vendor/
composer.lock

# Archivos de configuración local
src/config/database.local.php
.env
.env.local
config.php.local

# Logs y archivos temporales
/logs/
*.log
error_log
access_log
/tmp/
/temp/
/cache/
/sessions/

# Archivos del sistema operativo
.DS_Store
Thumbs.db
desktop.ini
.Spotlight-V100
.Trashes
ehthumbs.db
Desktop.ini

# IDEs y editores
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
*.swp
*.swo
*~
.project
.settings/
nbproject/
.vs/

# Archivos de respaldo
*.bak
*.backup
*.old
*.orig
*.~*
*.tmp

# Archivos de base de datos locales
*.sql
*.sql.gz
*.sql.zip
database/backup/
database/migrations/local/
database/local_*

# Archivos subidos por usuarios
public/uploads/
public/assets/uploads/
uploads/

# Archivos de testing
test_*.php
*_test.php
tests/coverage/
phpunit.xml
.phpunit.result.cache
.php_cs.cache
.phpunit.cache

# Archivos de documentación local
docs/local/
*.todo
TODO.md
REPORTE_*.md
NOTES.md

# Node modules y bundlers
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
yarn.lock

# Archivos de construcción
/build/
/dist/
/out/

# Archivos de configuración de servidor local
.htaccess.local
.htaccess.backup
apache.conf
nginx.conf

# Archivos de sesión PHP
sess_*
php_session_*

# Archivos específicos del proyecto que no deben versionarse
database/*.local.sql
src/config/app.local.php
src/config/local.*

# Ignorar archivos de desarrollo/debug
debug_*.php
fix_*.php
test_*.sql
add_*.sql
update_*.sql
create_*.php
check_*.php
quick_*.php
temp_*.php
SISTEMA_*.md
DEPLOYMENT_CHECKLIST.md

# Ignorar archivos de caché y optimización
*.cache
*.map
*.min.js.map
*.min.css.map

# Ignorar archivos de configuración de XAMPP/localhost
my.ini
my.cnf

# Ignorar archivos generados automáticamente
assets/js/*compiled.js
assets/css/*compiled.css

# Ignorar archivos de errores de PHP
errors.log
php_errors.log

# Ignorar archivos de exportación
*.csv
*.xlsx
*.pdf

# Ignorar archivos temporales de navegador
*.tmp
*.temp
